{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build && tsc-alias",
    "dev": "nodemon --legacy-watch --exec 'node --inspect=0.0.0.0:9229 -r ts-node/register -r tsconfig-paths/register' src/server.ts",
    "start": "ts-node -r tsconfig-paths/register dist/api/src/server.js",
    "test": "jest --coverage",
    "format": "prettier --write .",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix ",
    "start-event-worker": "ts-node -r tsconfig-paths/register src/core/event/event.worker.ts",
    "migrate": "docker exec -it express-next-trpc-boilerplate-backend-1 sh -c \"npx prisma migrate dev && npx prisma generate\" && npx prisma generate",
    "seed": "docker exec -it express-next-trpc-boilerplate-backend-1 sh -c \"npx ts-node -r tsconfig-paths/register prisma/seed/seed.ts\"",
    "generate:feature": "ts-node -r tsconfig-paths/register scripts/generateFeature.ts",
    "generate:event-types": "ts-node -r tsconfig-paths/register scripts/generateEventTypes.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.21.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@trpc/client": "^11.0.0-rc.660",
    "@trpc/server": "^11.0.0-rc.660",
    "@types/winston": "^2.4.4",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "path": "^0.12.7",
    "socket.io": "^4.8.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^5.21.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
